ODOO_MAJOR_VERSION=17
ODOO_MINOR_VERSION=0
GITHUB_USER=USERNAME
GITHUB_EMAIL=EMAIL
GITHUB_TOKEN=ghp_TOKEN
ODOO_COMMUNITY_REPOSITORY=github.com/odoo/odoo
ODOO_ENTERPRISE_REPOSITORY=github.com/odoo/enterprise
GEOIPUPDATE_ACCOUNT_ID=example
GEOIPUPDATE_LICENSE_KEY=example
POSTGRES_HOST=postgres
POSTGRES_USER=odoo
POSTGRES_PASSWORD=odoo
POSTGRES_DB=odoo
POSTGRES_SSL_MODE=disable
REDIS_HOST=redis
REDIS_PASSWORD=redis
# Redis TLS/SSL configuration
# REDIS_SSL can be 'true' or 'false' (default 'false')
REDIS_SSL=false

# When using TLS, set REDIS_SSL=true and configure the following parameters as needed
# REDIS_SSL_CA_CERTS - Path to CA certificates file (optional)
# REDIS_SSL_CERTFILE - Path to client certificate file (optional)
# REDIS_SSL_KEYFILE - Path to client key file (optional)
# REDIS_SSL_CHECK_HOSTNAME - 'true' or 'false' to enable/disable hostname verification (default 'false')
# REDIS_SSL_CERT_REQS - 'none', 'optional', or 'required' (default 'required')

# Example TLS configuration:
# REDIS_SSL=true
# REDIS_SSL_CA_CERTS=/path/to/ca_certs.pem
# REDIS_SSL_CERTFILE=/path/to/client_cert.pem
# REDIS_SSL_KEYFILE=/path/to/client_key.pem
# REDIS_SSL_CHECK_HOSTNAME=true
# REDIS_SSL_CERT_REQS=required
